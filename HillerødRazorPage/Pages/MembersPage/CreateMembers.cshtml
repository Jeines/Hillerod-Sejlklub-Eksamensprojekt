@page
@model HillerødRazorPage.Pages.MembersPage.CreateMembersModel
@{
}
<h1>Lav et nyt medlem</h1>

<form method="post">
	<div class="form-group">
		<label asp-for="@Model.member.Id" class="control-label"></label>
		<input asp-for="@Model.member.Id" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.member.Name" class="control-label"></label>
		<input asp-for="@Model.member.Name" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.member.LastName" class="control-label"></label>
		<input asp-for="@Model.member.LastName" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.member.PhoneNumber" class="control-label"></label>
		<input asp-for="@Model.member.PhoneNumber" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.member.Email" control-label"></label>
		<input asp-for="@Model.member.Email" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.member.Address" class="control-label"></label>
		<input asp-for="@Model.member.Address" class="form-control" />
	</div>
	
	<p>
		<div class="form-group">
			<input type="submit" value="Create" class="btn btn-primary" />
		</div>
	</p>
</form>